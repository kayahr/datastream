{
    "$schema": "http://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es2015.symbol.wellknown", "es2015.reflect", "es2015.iterable", "es2015.collection", "es2015.promise",
            "es2015.core", "es5", "dom"
        ],
        "moduleResolution": "node",
        "module": "commonjs",
        "rootDir": "src",
        "outDir": "lib",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "sourceMap": true,
        "declaration": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "skipLibCheck": true,
        "importHelpers": true,
        "downlevelIteration": true,
        "preserveSymlinks": true
    }
}
