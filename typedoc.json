{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "src/main/core.ts",
        "src/main/node.ts"
    ],
    "basePath": "src/main",
    "entryPointStrategy": "resolve",
    "out": "lib/apidoc",
    "excludePrivate": true,
    "excludeExternals": true,
    "excludeInternal": true,
    "treatWarningsAsErrors": true,
    "validation": true,
    "gitRevision": "main"
}
